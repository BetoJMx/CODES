  CREATE TABLE "XXODM_PAY_COMP_NOM_ERROR_LOG_T" 
   (	"ID_ERROR" NUMBER GENERATED ALWAYS AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"PROCESS_ID" NUMBER, 
	"ERROR" VARCHAR2(1000), 
	"FECHA" DATE, 
	"CREATED_BY" VARCHAR2(100), 
	"CREATION_DATE" DATE DEFAULT SYSDATE - 6/24, 
	"LAST_UPDATED_BY" VARCHAR2(100), 
	"LAST_UPDATE_DATE" DATE DEFAULT SYSDATE - 6/24, 
	"MENSAJE" CLOB
   ) ;
