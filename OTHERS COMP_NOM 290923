--OTHERS COMP_NOM 290923
CREATE SEQUENCE  "ODM_PAY_COMP_NOM_JOB_S"  MINVALUE 1 MAXVALUE 9999999999999999999999999 INCREMENT BY 1 START WITH 100 NOCACHE  NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL;
CREATE SEQUENCE  "ODM_PAY_COMP_NOM_PROC_S"  MINVALUE 1 MAXVALUE 9999999999999999999999999 INCREMENT BY 1 START WITH 100 NOCACHE  NOORDER  NOCYCLE  NOKEEP  NOSCALE  GLOBAL;

CREATE TABLE "ODM_PAY_COMP_NOM_CLOB_DATA" (	
    "CONTENIDO" CLOB, 
    "PROCESS_ID" NUMBER, 
    "F_EJECUCION" DATE DEFAULT SYSDATE, 
    CONSTRAINT "ODM_COMP_NOM_CLOB_DATA_PK" PRIMARY KEY ("PROCESS_ID")
    USING INDEX  ENABLE
) ;

